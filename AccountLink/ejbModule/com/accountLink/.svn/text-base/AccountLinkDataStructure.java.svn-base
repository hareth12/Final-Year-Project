package com.accountLink;

import java.io.Serializable;

public class AccountLinkDataStructure implements Serializable {
	private long payeeAccount;
	private String accountLinkHash;
	private long payerAccount;
	private String payerAccountType;
	private String payeeAccountType;
	
	public AccountLinkDataStructure(long payeeAccount, String accountLinkHash){
		this.setAccountLinkHash(accountLinkHash);
		this.setPayeeAccount(payeeAccount);
	}

	public AccountLinkDataStructure() {
		// TODO Auto-generated constructor stub
	}

	public void setPayeeAccount(long payeeAccount) {
		this.payeeAccount = payeeAccount;
	}

	public long getPayeeAccount() {
		return payeeAccount;
	}

	public void setAccountLinkHash(String accountLinkHash) {
		this.accountLinkHash = accountLinkHash;
	}

	public String getAccountLinkHash() {
		return accountLinkHash;
	}

	public void setPayerAccount(long payerAccount) {
		this.payerAccount = payerAccount;
	}

	public long getPayerAccount() {
		return payerAccount;
	}

	public void setPayerAccountType(String payerAccountType) {
		this.payerAccountType = payerAccountType;
	}

	public String getPayerAccountType() {
		return payerAccountType;
	}

	public void setPayeeAccountType(String payeeAccountType) {
		this.payeeAccountType = payeeAccountType;
	}

	public String getPayeeAccountType() {
		return payeeAccountType;
	}
	
	
}
