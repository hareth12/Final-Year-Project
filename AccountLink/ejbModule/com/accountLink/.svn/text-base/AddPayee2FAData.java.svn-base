package com.accountLink;

public class AddPayee2FAData {
	private String indexHash;
	private long payeeAccount;
	private long payerAccount;
	private String preHash;
	private String hash2FA;
	private Boolean addPayee2FA1Success;
	private Boolean addPayee2FA2Success;
	
	public AddPayee2FAData(){
		this.addPayee2FA1Success=false;
		this.addPayee2FA2Success=false;
		this.hash2FA=null;
		this.payeeAccount=0;
		this.payerAccount=0;
		this.hash2FA=null;
		this.preHash=null;
	}
	
	public void setIndexHash(String indexHash) {
		this.indexHash = indexHash;
	}
	public String getIndexHash() {
		return indexHash;
	}
	public void setPayerAccount(long payerAccount) {
		this.payerAccount = payerAccount;
	}
	public long getPayerAccount() {
		return payerAccount;
	}
	public void setPayeeAccount(long payeeAccount) {
		this.payeeAccount = payeeAccount;
	}
	public long getPayeeAccount() {
		return payeeAccount;
	}
	public void setPreHash(String preHash) {
		this.preHash = preHash;
	}
	public String getPreHash() {
		return preHash;
	}
	public void setHash2FA(String hash2FA) {
		this.hash2FA = hash2FA;
	}
	public String getHash2FA() {
		return hash2FA;
	}
	public void setAddPayee2FA1Success(Boolean addPayee2FA1Success) {
		this.addPayee2FA1Success = addPayee2FA1Success;
	}
	public Boolean getAddPayee2FA1Success() {
		return addPayee2FA1Success;
	}
	public void setAddPayee2FA2Success(Boolean addPayee2FA2Success) {
		this.addPayee2FA2Success = addPayee2FA2Success;
	}
	public Boolean getAddPayee2FA2Success() {
		return addPayee2FA2Success;
	}
}

