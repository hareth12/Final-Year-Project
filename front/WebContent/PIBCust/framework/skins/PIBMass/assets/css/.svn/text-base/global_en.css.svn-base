*{margin:0;padding:0;}
body{ color: #444; font-family: Arial,Helvetica,sans-serif; font-size: 11px; line-height: 18px; }
textarea{ color: #444; font-family: Arial,Helvetica,sans-serif; font-size: 11px; line-height: 18px; padding:3px;}
a{text-decoration:none;color:#0000ff;cursor: pointer;}
a:hover{text-decoration:none;}
