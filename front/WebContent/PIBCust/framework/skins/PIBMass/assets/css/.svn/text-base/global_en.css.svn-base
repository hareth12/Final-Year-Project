/* Layout */
.wrapper{width:980px;margin:0 auto;}
#header{border-bottom:1px solid #bbb;margin:0 20px;padding:10px 0 0;}
#overlayheader{border-bottom:1px solid #bbb;margin:0 20px;padding:10px 0 0;}
#header-left{float:left;width:70%;}
#header-right{float: right; display: inline; width: 200px; font-size: 10px; }
#menu{margin:10px 20px;}
#sidebar{width:170px;overflow:hidden;font-size:12px;float:left;}
#content{clear:both;margin:0 20px;}
#main{font-size:85%;float:right;width:765px;}

/* Header */
#site-logo-container,#site-logo{float:left;}
#site-welcome{position:relative;padding:30px 0 0 135px;}
#site-animation{float:right;position:relative;}
h1#uob-logo a{text-indent:-9999px;display:block;width:130px;background:url('../../assets/images/common/uob-logo.jpg') no-repeat;height:45px;}
h3#welcome-text{text-indent:-9999px;display:block;width:125px;background:url('../../assets/images/common/text_welcome-to-uob-singapore.gif') no-repeat;height:15px;}
#main-menu ul li a{ color: #A5A5A5; font-size: 13px; font-weight: bold; display: block; border-bottom: 2px solid #46699F; line-height: 25px; }
#main-menu ul li a:hover{border-bottom:2px solid #990000;}
#main-menu ul li{float:left;font-size:13px;}
#main-menu ul li#personal-banking{width:150px;color:#006699;font-weight:bold;margin:15px 0 0 0px;}
#main-menu ul li#corporate-banking{width:245px;margin:10px 0 0 10px;}
#main-menu ul li#investor-relations{width:165px;margin:10px 0 0 10px;}
#main-menu ul li#about-uob{width:140px;margin:10px 0 0 10px;}
#main-menu ul#banking-type{float:right;}
#main-menu ul li.banking-menu{margin:0 5px;padding:10px 0 5px;font-size:11px;}
#main-menu ul li.banking-menu a{color:#000;display:block;border-bottom:none !important;background:url('../../assets/images/common/buttons/btn_bg_off_pbanking.gif') no-repeat bottom;line-height:30px;}
#main-menu ul li.banking-menu a:hover{background:url('../../assets/images/common/buttons/btn_bg_on.gif') repeat-x bottom;}


/* leftnav and sidebar */
/* #sidebar a{display:block;padding-left:10px;} vC091*/
#sidebar a{display:block;padding-left:5px;}
#sidebar ul{padding:0 0 10px 10px;}
#sidebar ul.nopad{padding:0 0 3px 0px;}
#sidebar ul li.collapsed{background:url('../../assets/images/common/icon/icon_collapse.gif') no-repeat;background-position:0 5px;}
#sidebar ul li.expanded{background:url('../../assets/images/common/icon/close.gif') no-repeat;background-position:0 5px;}
#sidebar ul li.arrowed{background:url('../../assets/images/common/icon/icon_arrow_left_1stlevel_light.gif') no-repeat;background-position:0 5px;}
#sidebar ul li.hidden{display:none;}
#sidebar ul li{display:inline;}
#sidebar .sidebar-panel{padding:0 5px;margin-bottom:2em;}
#sidebar a:hover{text-decoration:underline;}


/* buttons */
.button a{text-indent:-9999px;display:block;}
.button-edit{float:right;line-height:22px;padding:4px 5px;}
.button-alignR{float:right;padding:0;position:static}
.button-alignR input{padding:0px;}
/*jc*/
/* .button-alignL{float:left;padding:0;position:static} */
/*jc*/
.button-alignL{float:left;}
.button-alignL input{padding:0px 0px;}
#page-actions{float:right;padding:5px 0;}
#page-actions ul li a{text-indent:-9999px;display:block;}
#button-email{background:url('../../assets/images/common/icon/icon_secure-email2.gif') no-repeat;width:23px;height:23px;}
#button-home{background:url('../../assets/images/common/icon/icon_home2.gif') no-repeat;width:23px;height:23px;}
